<blueprint type="INDI" name="Complete"><![CDATA[ 
<p>Individual <font color=blue><b><prop path=INDI></b></font></p>
<table>
 <tr><td></td><td></td><td rowspan=5><prop path=INDI:OBJE:FILE maxh=25></td></tr> 
 <tr>
  <td><b>Name </b></td>
  <td><i><prop path=INDI:NAME></i></td>
 </tr><tr>
  <td><b>Sex  </b></td>
  <td><prop path=INDI:SEX img=yes txt=no></td>
 </tr><tr>
  <td><b>Birth</b></td>
  <td><prop path=INDI:BIRT:DATE img=yes>, 
   <u><prop path=INDI:BIRT:PLAC></u></td>
 </tr><tr>
  <td><b>Addr </b></td>
  <td><prop path=INDI:RESI:ADDR><br>
      <prop path=INDI:RESI:ADDR:CITY><br>
      <prop path=INDI:RESI:POST></u>
  </td>
 </tr> 
</table>
]]></blueprint>


